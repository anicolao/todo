		"vite": "^4.0.0",
		"vitest": "^0.30.1",
		"webdriverio": "^8.8.0"
	},
	"type": "module",
	"dependencies": {
		"@capacitor-firebase/authentication": "^1.3.0",
		"@capacitor/android": "^4.7.0",
		"@capacitor/core": "^4.7.0",
		"@capacitor/ios": "^4.7.0",
		"@reduxjs/toolkit": "^1.8.5",
